{
    "name": "Homework Calendar Integration",
    "version": "1.0",
    "description": "Integrate your homework assignments to your google calendar",
    "manifest_version": 2,
    "key": "",
    "oauth2":{
        "client_id": "838303626781-9dd2jkscnnrto6spsb4esbo97dfq1p9h.apps.googleusercontent.com",
        "scopes": [
            "https://googleapis.com/auth/calendar.settings.readonly",
            "https://googleapis.com/auth/calendar.events"
        ]
    },
    "permissions":[
        "activeTab", "declarativeContent", "identity", "storage"
    ],
    "page_action": {
        "default_popup": "popup.html"
    },
    "background": {
      "scripts": [
        "background.js"
      ],
      "persistent": false
    }
}

